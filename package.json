{
  "name": "ctg-suit",
  "version": "1.12.10",
  "license": "lianmed-1.0.0",
  "private": true,
  "scripts": {
    "start:dev": "concurrently  'yarn start' 'yarn run dev:main'",
    "start": "cross-env PORT=1702 BROWSER=none umi dev",
    "start:render": "cross-env PORT=1702 TARGET=0 umi dev",
    "start:main": "run-electron .",
    "dev:main": "yarn gulp && run-electron .",
    "_dev:main": "wsl make && run-electron .",
    "gulp": "gulp --silent",
    "build:main": "tsc --project ./app/tsconfig.json",
    "build": "ts-node --project main/tsconfig.json ./scripts/build",
    "build:render": "cross-env BROWSER=none umi build",
    "build:runtime": "umi build",
    "codepush:browser": "node ./scripts/codepush.js --dir=.tmp  --base app --target=browser",
    "codepush:desktop": "node ./scripts/codepush.js --dir=.tmp  --base=app --target=desktop",
    "deploy:browser": "yarn vh && yarn gulp && yarn build:runtime && yarn codepush:browser",
    "deploy:desktop": "yarn vh && yarn gulp  && yarn build:render && yarn codepush:desktop",
    "_deploy:desktop": "yarn vh && yarn gulp  && yarn codepush:desktop",
    "test": "umi test",
    "lint": "eslint --ext .js src mock tests",
    "rebuild": "electron-rebuild -m ./app",
    "pp": "electron-builder --dir =./app",
    "pack": "npm run build:render && npm run rebuild && electron-builder --dir =./app",
    "pack:ia32": "npm run build:render && npm run rebuild && electron-builder  --ia32 --dir =./app",
    "pack-linux": "npm run build:render && npm run rebuild && electron-builder build --linux",
    "dist": "electron-builder",
    "dist:ia32": "electron-builder --ia32",
    "precommit": "lint-staged",
    "update:lianmed": "yarn upgrade @lianmed/lmg @lianmed/components @lianmed/utils @lianmed/request",
    "link:lianmed": "npm link @lianmed/lmg @lianmed/components @lianmed/utils @lianmed/request @lianmed/pages",
    "vh": "ts-node --project ./main/tsconfig.json ./scripts/versionHandler "
  },
  "main": "./app/main/index.js",
  "dependencies": {
    "antd": "^3.19.5",
    "bezier-easing": "^2.1.0",
    "braft-editor": "^2.3.8",
    "braft-utils": "^3.0.12",
    "dva": "^2.6.0-beta.6",
    "electron-store": "^5.0.0",
    "qrcode": "^1.4.4",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-color": "^2.17.3",
    "react-dom": "^16.8.6",
    "react-highlight-words": "^0.16.0",
    "react-pdf": "^4.1.0",
    "run-electron": "^1.0.0",
    "store": "^2.0.12",
    "umi-request": "^1.2.2"
  },
  "devDependencies": {
    "@chatie/tsconfig": "^0.6.1",
    "@types/gulp": "^4.0.6",
    "@types/gulp-typescript": "^2.13.0",
    "@types/lodash": "^4.14.144",
    "@types/request": "^2.48.3",
    "@types/webpack": "^4.41.2",
    "asar": "^2.0.3",
    "babel-eslint": "^9.0.0",
    "compressing": "^1.5.0",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.0",
    "electron": "^5.0.13",
    "electron-builder": "^21.1.5",
    "electron-is-dev": "^1.1.0",
    "electron-rebuild": "^1.8.5",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglyfly": "^1.4.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mockjs": "^1.0.1-beta3",
    "postcss-px-to-viewport": "^1.1.1",
    "progress": "^2.0.3",
    "react-test-renderer": "^16.7.0",
    "roadhog-api-doc": "^1.1.2",
    "shx": "^0.3.2",
    "slash": "^3.0.0",
    "ts-node": "^8.5.4",
    "umi": "^2.7.7",
    "umi-plugin-react": "^1.8.4",
    "webpack": "^4.41.0",
    "webpack-chain": "^6.0.0",
    "yargs-parser": "^16.1.0"
  },
  "build": {
    "productName": "ctg-suit",
    "appId": "lian-med-ctgsuit",
    "copyright": "Copyright © 2019 lian-med",
    "compression": "maximum",
    "asar": true,
    "directories": {
      "output": "release"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "menuCategory": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": {
    "name": "Lian-Med",
    "email": "lian-med@163.com",
    "hospital": "xxx 医院",
    "date": "2019-12-22"
  }
}